  unparse_cocci.cmo
  unparse_c.cmo
  unparse_hrule.cmo
  parsing_recovery_c.cmo
  parsing_consistency_c.cmo
  danger.cmo
  parse_c.cmo
  type_c.cmo
  cpp_ast_c.cmo
  type_annoter_c.cmo
  comment_annotater_c.cmo
  compare_c.cmo
  test_parsing_c.cmo

engine/cocciengine.cma:
  flag_matcher.cmo
  lib_engine.cmo
  pretty_print_engine.cmo
  check_exhaustive_pattern.cmo
  check_reachability.cmo
